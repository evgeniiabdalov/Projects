<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="281.0" prefWidth="661.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.LoadFileGUI">
   <children>
      <Button fx:id="loadAgent" layoutX="14.0" layoutY="31.0" mnemonicParsing="false" onAction="#setAgentFileChooser" text="Load agents" />
      <Button fx:id="loadGraph" layoutX="14.0" layoutY="88.0" mnemonicParsing="false" onAction="#setGraphFileChooser" prefHeight="26.0" prefWidth="98.0" text="Load graph" />
      <Button fx:id="loadPlans" layoutX="14.0" layoutY="141.0" mnemonicParsing="false" onAction="#setPlanFileChooser" prefHeight="26.0" prefWidth="98.0" text="Load plans" />
      <TextField fx:id="loadAgentText" disable="true" layoutX="129.0" layoutY="31.0" prefHeight="26.0" prefWidth="499.0" />
      <TextField fx:id="loadGraphText" disable="true" layoutX="129.0" layoutY="88.0" prefHeight="26.0" prefWidth="499.0" />
      <TextField fx:id="loadPlansText" disable="true" layoutX="129.0" layoutY="141.0" prefHeight="26.0" prefWidth="499.0" />
      <Button fx:id="loadFileSubmit" layoutX="426.0" layoutY="231.0" mnemonicParsing="false" onAction="#setLoadFileSubmit" prefHeight="26.0" prefWidth="150.0" text="SUBMIT" />
      <TextField fx:id="inputNameText" layoutX="133.0" layoutY="189.0" prefHeight="26.0" prefWidth="499.0" />
      <Label layoutX="14.0" layoutY="192.0" prefHeight="26.0" prefWidth="88.0" text="Input name" />
   </children>
</AnchorPane>
